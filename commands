~/prod/taski-docker/backend
docker build -t taski_backend .
docker build -t zmarik/taski_backend:latest .
docker run --name taski_backend_container -p 8001:8000 -v sqlite_data:/data taski_backend

~/prod/taski-docker/frontend
docker build -t taski_frontend . 
docker build -t zmarik/taski_frontend:latest .
docker run --rm -it -p 8002:8000 --name taski_frontend_test taski_frontend

docker push zmarik/taski_backend:latest
docker push zmarik/taski_frontend:latest

docker network connect django-network db 