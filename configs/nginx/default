server {
    server_name taski.theworkpc.com;
    
    location / {
        proxy_set_header Host $http_host;
        proxy_pass http://127.0.0.1:8000;
    }



    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/kittygram.theworkpc.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/kittygram.theworkpc.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    server_name kittygram.theworkpc.com;

    location / {
        root   /var/www/kittygram;
        index  index.html index.htm;
        try_files $uri /index.html;
    }

    location /api/ {
        client_max_body_size 20M;
        proxy_pass http://127.0.0.1:8080;
    }

    location /admin/ {
        client_max_body_size 20M;
        proxy_pass http://127.0.0.1:8080;
    }


    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/kittygram.theworkpc.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/kittygram.theworkpc.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server {
    if ($host = kittygram.theworkpc.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name kittygram.theworkpc.com;
    listen 80;
    return 404; # managed by Certbot


}server {
    if ($host = taski.theworkpc.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    server_name taski.theworkpc.com;
    listen 80;
    return 404; # managed by Certbot


}
